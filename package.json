{
  "name": "san-studio",
  "version": "0.0.1",
  "author": "DmitriVanGuard <dimavanguard@gmail.com>",
  "private": true,
  "engines": {
    "node": "<=14"
  },
  "lint-staged": {
    "*.{js,ts,svelte,css,md,mdx}": [
      "prettier --write"
    ]
  },
  "precommit": [
    "test"
  ],
  "prepush": [
    "test"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "serve": "vite preview",
    "test": "jest",
    "validate": "svelte-check",
    "validate:errors": "yarn validate --threshold error",
    "lib:prepare": "node scripts/prepare.js && tsc --p ./scripts/tsconfig.json && node scripts/cleanup.js",
    "lib": "npm run lib:prepare && node scripts/lib.js",
    "install": "npm run lib",
    "postinstall": "patch-package",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@santiment-network/chart": "^0.10.12",
    "@sveltejs/vite-plugin-svelte": "^1.0.0-next.13",
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "@typescript-eslint/parser": "^4.18.0",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-svelte3": "^3.1.2",
    "eslint-svelte3-preprocess": "0.0.5",
    "husky": "^7.0.4",
    "jest": "^26.6.3",
    "lint-staged": "^12.3.7",
    "marked": "^4.0.12",
    "medium-editor": "^5.23.3",
    "node-fetch": "^2.6.1",
    "patch-package": "^6.4.7",
    "prettier": "^2.6.2",
    "prettier-plugin-svelte": "^2.7.0",
    "svelte": "^3.48.0",
    "ts-jest": "^26.5.6",
    "turndown": "^7.1.1",
    "vite": "^2.5.1",
    "vite-plugin-ts-checker": "^0.1.5"
  },
  "dependencies": {
    "mathjs": "^9.4.4",
    "san-webkit": "https://github.com/santiment/san-webkit#6b0b80a",
    "svelte-check": "^1.5.2",
    "svelte-preprocess": "^4.7.4",
    "svelte-preprocess-cssmodules": "https://github.com/DmitriVanGuard/svelte-preprocess-cssmodules#3b24f22",
    "typescript": "^4.2.4",
    "phoenix": "^1.6.6"
  },
  "peerDependencies": {
    "svelte": "^3.46.3",
    "@santiment-network/chart": "^0.10.12",
    "san-webkit": "https://github.com/santiment/san-webkit.git",
    "canvas": "^2.8.0",
    "phoenix": "^1.6.6"
  }
}
